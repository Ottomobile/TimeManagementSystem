@model IEnumerable<TimeManagementSystem.Models.SubscribeToUser>

@{
    ViewData["Title"] = "Managers Added";
}

<h2>Managers Added</h2>

<p>
    <a asp-action="Add">Add Manager</a>
</p>
<div class="table-responsive">
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.ManagingUser)
                </th>
                <th>
                    Action
                </th>
            </tr>
        </thead>
        <tbody>
    @foreach (var item in Model) {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ManagingUser)
                </td>
                <td>
                    <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
                </td>
            </tr>
    }
        </tbody>
    </table>
</div>
